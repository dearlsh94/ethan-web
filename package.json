{
  "name": "ethan-web",
  "version": "1.5.1",
  "private": true,
  "description": "ethan-web",
  "author": "SeungHwan Ethan Lee",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "dev": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "hosting": "yarn && yarn build",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
